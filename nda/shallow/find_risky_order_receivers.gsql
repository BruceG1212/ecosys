use graph MyGraph
#SetAccum dedup, print run-time attribute with object
drop query FIND_RISKY_RECEIVERS 
CREATE QUERY FIND_RISKY_RECEIVERS(int cutoff=3) Syntax v2{

  SetAccum<VERTEX> @order_senders;

  risky_receivers = SELECT t
                    FROM Account:s -(send_bonus>:e)- Account:t
                    ACCUM t.@order_senders += s
                    HAVING t.@order_senders.size() > cutoff;

  PRINT  risky_receivers;
}






