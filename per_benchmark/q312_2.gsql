use graph ldbc_snb

drop query q312_2
CREATE QUERY q312_2 () SYNTAX _v2 {
  MaxAccum<INT> @oldest;
  //find how many post/comment liked by a person
  V = SELECT s
      FROM Tag:t -(<HAS_TAG)- :f - (CONTAINER_OF>)-Post:p- (<LIKES)-Person:s
      PER (s,t)
      ACCUM s.@oldest += t.id
      ORDER BY s.@oldest
      LIMIT 2;

  Print V;

}

