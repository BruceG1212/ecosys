use graph ldbc_snb

drop query q314_2
CREATE QUERY q314_2 () SYNTAX _v2 {
  MaxAccum<INT> @oldest;
  //find how many post/comment liked by a person
  V = SELECT f
      FROM :f - (CONTAINER_OF>)-Post:p- (<LIKES)-Person:s
      PER (s,f)
      ACCUM f.@oldest += datetime_to_epoch(s.creationDate)
      ORDER BY f.@oldest
      LIMIT 2;

  Print V;

}

