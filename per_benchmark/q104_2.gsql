use graph ldbc_snb

drop query q104_2
CREATE QUERY q104_2 () SYNTAX _v2 {

  MaxAccum<Datetime> @@oldest;

  //find how many post/comment liked by a person
  V = SELECT tgt
      FROM Person:s- (LIKES>)-Comment:tgt
      PER(tgt)
      ACCUM @@oldest += tgt.creationDate;

  Print @@oldest;

}

//install query q104_2
//run query q104_2()
