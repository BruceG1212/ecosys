use graph ldbc_snb

drop query q403_2
CREATE QUERY q403_2 () SYNTAX _v2 {
  SumAccum<int> @@forum_cnt;
  V = SELECT p
      FROM Forum:f-(CONTAINER_OF>)-Post:p-(<LIKES)-Person:s
      PER (p)
      ACCUM @@forum_cnt += datetime_to_epoch(p.creationDate);

  Print @@forum_cnt;

}

