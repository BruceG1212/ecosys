use graph ldbc_snb
drop query q322
CREATE QUERY q322 () SYNTAX _v2 {
  MaxAccum<INT> @oldest;
  V = SELECT t 
      FROM Person:s -(LIKES>)-Comment:t
      PER (s,t)
      ACCUM t.@oldest += datetime_to_epoch(s.creationDate)
      POST_ACCUM t.@oldest+= datetime_to_epoch(t.creationDate)
      ORDER BY t.@oldest
      LIMIT 2;
  Print  V;
}

