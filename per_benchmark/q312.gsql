use graph ldbc_snb

drop query q312
CREATE QUERY q312 () SYNTAX _v2 {
  MaxAccum<INT> @oldest;
  //find how many post/comment liked by a person
  V = SELECT s
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f -(HAS_TAG>)-:t
      PER (s,t)
      ACCUM s.@oldest += t.id
      ORDER BY s.@oldest
      LIMIT 2;

  Print V;

}

