use graph ldbc_snb

drop query q306
CREATE QUERY q306 () SYNTAX _v2 {
  MaxAccum<String> @@tagname;
  V = SELECT t
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f - (HAS_TAG>) - :t
      PER (s,t)
      ACCUM @@tagname += t.name;
  Print @@tagname;
}

