use graph ldbc_snb

drop query qk3_2
CREATE QUERY qk3_2 () SYNTAX _v2 {

  SumAccum<int> @@comment_cnt;

  V = SELECT s
      FROM Comment:t- (<REPLY_OF*)-:s
      WHERE t.id == 13194144951001
      PER (s)
      ACCUM @@comment_cnt += 1;

  Print @@comment_cnt;

}

