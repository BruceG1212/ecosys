use graph ldbc_snb

drop query q317_2
CREATE QUERY q317_2 () SYNTAX _v2 {
  MaxAccum<INT> @@oldest;
  //find how many post/comment liked by a person
  V = SELECT f
      FROM :f - (CONTAINER_OF>)-Post:p- (<LIKES)-Person:s
      PER (s,f)
      ACCUM @@oldest += datetime_to_epoch(s.creationDate);

  Print @@oldest;

}

