use graph ldbc_snb

drop query q317
CREATE QUERY q317 () SYNTAX _v2 {
  MaxAccum<INT> @@oldest;
  //find how many post/comment liked by a person
  V = SELECT f
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f
      PER (s,f)
      ACCUM @@oldest += datetime_to_epoch(s.creationDate);

  Print @@oldest;

}

