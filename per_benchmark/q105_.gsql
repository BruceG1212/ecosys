use graph ldbc_snb
drop query q105_

CREATE QUERY q105_ () SYNTAX v1 {
  MaxAccum<Datetime> @@oldest;

VS_f = {Forum.*};
VS__1 =
	select p
	from   VS_f:f -(CONTAINER_OF:x)-> Post:p;

XXX1 =
	select s
	from   VS__1:p -(LIKES_REVERSE:x_2)-> Person:s;


V =
	select	f
	from   XXX1:f
	accum       @@oldest += f.creationDate
		;

  Print @@oldest;

}

//install query q105_
