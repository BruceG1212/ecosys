use graph ldbc_snb
drop query q319_2
CREATE QUERY q319_2 () SYNTAX _v2 {
  MaxAccum<INT> @@oldest;
  V = SELECT t 
      FROM Comment:t-(<LIKES)-Person:s 
      PER (s,t)
      ACCUM @@oldest += datetime_to_epoch(s.creationDate),
      @@oldest += datetime_to_epoch(t.creationDate);
  Print  @@oldest;

}

