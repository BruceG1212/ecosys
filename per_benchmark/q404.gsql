use graph ldbc_snb

drop query q404
CREATE QUERY q404 () SYNTAX _v2 {
  SumAccum<int> @@tag_cnt;
  V = SELECT f
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f - (HAS_TAG>) - :t
      PER (f)
      ACCUM @@tag_cnt += datetime_to_epoch(f.creationDate);
  Print @@tag_cnt;
}

