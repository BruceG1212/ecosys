use graph ldbc_snb

drop query q111
CREATE QUERY q111 () SYNTAX _v2 {


  MaxAccum<Datetime> @oldest;

  //find how many post/comment liked by a person
  V = SELECT f
      FROM Forum:f- (CONTAINER_OF>)-Post:p - (<LIKES) - Person:s
      PER (f)
      ACCUM f.@oldest += f.creationDate;

  Print V.size();

}

