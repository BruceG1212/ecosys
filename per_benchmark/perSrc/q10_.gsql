use graph ldbc_snb
drop query q10_

CREATE QUERY q10_ () SYNTAX v1 {

  MaxAccum<Datetime> @oldest;

VS_s = {Person.*};
VS__1 =
	select tgt
	from   VS_s:s -(LIKES:x)-> Comment:tgt;

V =
	select	tgt
	from   VS__1:tgt
	accum       tgt.@oldest += tgt.creationDate
		;

  Print  V.size();

}

install query q10_
