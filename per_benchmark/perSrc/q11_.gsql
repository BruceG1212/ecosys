use graph ldbc_snb
drop query q11_
CREATE QUERY q11_ () SYNTAX v1 {


MaxAccum<Datetime> @oldest;

VS_s = {Person.*};
VS__1 =
	select p
	from   VS_s:s -(LIKES:x)-> Post:p;

VS__2 =
	select f
	from   VS__1:p -(CONTAINER_OF_REVERSE:x_2)-> _:f;

V =
	select	f
	from   VS__2:f
	accum       f.@oldest += f.creationDate
		;


  Print V.size();

}

install query q11_
