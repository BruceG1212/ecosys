use graph ldbc_snb

drop query q102_2
CREATE QUERY q102_2 () SYNTAX _v2 {

  SumAccum<int> @@person_cnt;

  //find how many post/comment liked by a person
  V = SELECT s 
      FROM Forum:f- (CONTAINER_OF>)- Post:p-(<LIKES) - Person:s
      PER (s) 
      ACCUM @@person_cnt += 1;

  Print @@person_cnt;

}

//install query q102_2
