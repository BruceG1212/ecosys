use graph ldbc_snb

drop query qk33_2
CREATE QUERY qk33_2 () SYNTAX _v2 {

  SumAccum<int> @@comment_cnt;

  V = SELECT s
      FROM Comment:t- (<REPLY_OF*)-:s
      WHERE t.id == 13194144951001
      PER (s,t)
      ACCUM @@comment_cnt += 1;

  Print @@comment_cnt;

}

