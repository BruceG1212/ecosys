use graph ldbc_snb

drop query q106_2
CREATE QUERY q106_2 () SYNTAX _v2 {

  MaxAccum<String> @@tagname;

  //find how many post/comment liked by a person
  V = SELECT t
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f - (HAS_TAG>) - _:t
      PER (t) 
      ACCUM @@tagname += t.name;

  Print @@tagname;

}

//install query q106_2
//run query q2()
