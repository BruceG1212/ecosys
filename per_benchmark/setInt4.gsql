use graph ldbc_snb
drop query setInt4
CREATE QUERY setInt4 () SYNTAX v1 {

  SetAccum<int> @personSet;

  Start = {Person.*};

  //find how many post/comment liked by a person
  V = SELECT t 
      FROM Start:s- (LIKES)->Comment:t
      ACCUM t.@personSet += s.id
      ORDER BY t.@personSet.size()
      LIMIT 2;

  Print V;

}
