use graph ldbc_snb

drop query q305
CREATE QUERY q305 () SYNTAX _v2 {


  MaxAccum<Datetime> @@oldest;

  //find how many post/comment liked by a person
  V = SELECT f
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-Forum:f
      PER (s,f)
      ACCUM @@oldest += f.creationDate;

  Print @@oldest;

}

