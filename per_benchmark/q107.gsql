use graph ldbc_snb

drop query q107
CREATE QUERY q107 () SYNTAX _v2 {

  SumAccum<int> @comment_cnt;

  V = SELECT s 
      FROM Person:s- (LIKES>)-Comment:t
      WHERE s.firstName == "Viktor" AND s.lastName == "Akhiezer" 
      PER (s)
      ACCUM s.@comment_cnt += 1;

  Print V.size();

}

//install query q107
