use graph ldbc_snb

drop query q103
CREATE QUERY q103 () SYNTAX _v2 {

  SumAccum<int> @@person_cnt;

  V = SELECT s
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f - (HAS_TAG>) - :t
      PER (s)
      ACCUM @@person_cnt += 1;

  Print @@person_cnt;

}

#install query q103
#run query q2()
