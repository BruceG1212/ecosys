use graph ldbc_snb
drop query setUint3
CREATE QUERY setUint3 () SYNTAX v1 {

  SetAccum<uint> @personSet;

  Start = {Person.*};

  //find how many post/comment liked by a person
  V = SELECT s 
      FROM Start:s- (LIKES)->Comment:tgt
      ACCUM s.@personSet+=tgt.id
      ORDER BY s.@personSet.size()
      LIMIT 2;

  Print V;

}
