use graph ldbc_snb

drop query q318
CREATE QUERY q318 () SYNTAX _v2 {
  MaxAccum<INT> @@oldest;
  //find how many post/comment liked by a person
  V = SELECT t
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f -(HAS_TAG>)-:t
      PER (s,t)
      ACCUM @@oldest += s.id;

  Print @@oldest;

}

