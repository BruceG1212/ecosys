use graph ldbc_snb

drop query q321_2
CREATE QUERY q321_2 () SYNTAX _v2 {
  MaxAccum<INT> @@oldest;
  //find how many post/comment liked by a person
  V = SELECT t
      FROM Tag:t -(<HAS_TAG)- :f - (CONTAINER_OF>)-Post:p- (<LIKES)-Person:s
      PER (s,t)
      ACCUM @@oldest += datetime_to_epoch(s.creationDate),
      @@oldest += t.id;

  Print @@oldest;

}

