use graph ldbc_snb

drop query q403
CREATE QUERY q403 () SYNTAX _v2 {

  SumAccum<int> @@forum_cnt;

  V = SELECT p
      FROM Person:s- (LIKES>)-Post:p - (<CONTAINER_OF)-:f
      PER (p)
      ACCUM @@forum_cnt += datetime_to_epoch(p.creationDate);

  Print @@forum_cnt;

}

