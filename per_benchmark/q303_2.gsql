use graph ldbc_snb

drop query q303_2
CREATE QUERY q303_2 () SYNTAX _v2 {
  SumAccum<int> @@tag_cnt;
  V = SELECT t
      FROM Tag:t  -(<HAS_TAG)- Forum:f -(CONTAINER_OF>)- Post:p -(<LIKES)- Person:s
      PER(s,t)
      ACCUM @@tag_cnt += 1;
  Print @@tag_cnt;
}

