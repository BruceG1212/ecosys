use graph ldbc_snb

drop query qk3
CREATE QUERY qk3 () SYNTAX _v2 {

  SumAccum<int> @@comment_cnt;

  V = SELECT s
      FROM :s- (REPLY_OF>*)-Comment:t
      WHERE t.id == 13194144951001
      PER (s)
      ACCUM @@comment_cnt += 1;

  Print @@comment_cnt;

}

