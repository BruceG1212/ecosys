CREATE QUERY to_jsonarray(SET<STRING> src) FOR GRAPH ldbc_snb RETURNS(JSONARRAY) {
  BOOL needComma = False;
  STRING jsonStr = "[";

  FOREACH s IN src DO
    IF needComma THEN
      jsonStr += ",";
    ELSE
      needComma = True;
    END;
    jsonStr += "\"" + s + "\"";
  END;
  jsonStr += "]";

  RETURN parse_json_array(jsonStr);
}